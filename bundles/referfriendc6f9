var ReferFriend=function(){function f(){$("#addFriend, #addFriendMobile").click(function(n){var u=$("#friends-container [name='indexHidden']").length;n.preventDefault();$.get(r,{index:u},function(n){n.MaxNumberReached?$("#maxNumberReached").show():($("#friends-container").append(n),t(),i())})})}function e(){$("#removeFriend, #removeFriendMobile").click(function(n){var r=$("#friends-container [name='indexHidden']").length;n.preventDefault();$("#referral"+r).remove();t();i()})}function t(){var n=$("#friends-container [name='indexHidden']").length;o(n);$("#friends-container input[type='text']").keyup(function(){o(n)})}function o(n){var t=!1;$("#friends-container input[type='text']").each(function(){$(this).val()==""&&(t=!0)});t||n>=u?$("#addFriend, #addFriendMobile").attr("disabled","disabled"):$("#addFriend, #addFriendMobile").removeAttr("disabled")}function i(){var n=$("#friends-container [name='indexHidden']").length;n>1?$("#removeFriend, #removeFriendMobile").removeAttr("disabled"):$("#removeFriend, #removeFriendMobile").attr("disabled","disabled")}var n=this,r,u=1;return n.init=function(n,o){r=n;u=o;f();e();t();i()},n.onComplete=function(){f();e();t();i()},$("#addFriendMobile").click(function(){$(".friend-btn-mobile.remove").css({top:"calc(100% - 395px)",display:"block"})}),{init:n.init,onComplete:n.onComplete}}