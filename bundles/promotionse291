var Promotions=function(){var n=this;return n.init=function(){$("[data-category] > a").click(function(t){t.preventDefault();t.stopPropagation();var i=$(this);n.changeCategory(i)})},n.purchaseBonusAmountChanged=function(n){try{var r=$(n).val().replace(",","."),t=parseFloat(r),i=0,u=$("#IsNonSticky").val().toLowerCase();i=u=="true"?(t*parseFloat($(n).attr("data-percentage"))).toFixed(2):(t+t*parseFloat($(n).attr("data-percentage"))).toFixed(2);$("#bonusAmount").html(isNaN(i)?0:i)}catch(f){}},n.optIn=function(n){$("#promo"+n+"-OptIn").trigger("click")},n.changeCategory=function(n){var t=$(n),i=t.attr("href"),r=t.attr("data-href");$("#promotionsContainer").load(r,function(n){app.navigateToUrl(n,null,i);$("[data-category] > a.active").length>0?($("[data-category] > a").removeClass("active"),t.addClass("active")):($("[data-category]").removeClass("active"),t.parents("[data-category]").addClass("active"))})},n.initFlashy=function(n){$(n).flashy({showClass:"fx-fadeIn",hideClass:"fx-fadeOut",overlayClose:!0,videoAutoPlay:!0,galleryPrevNext:!1,galleryCounter:!1,gallery:!1,videoAutoPlay:!1})},{init:n.init,purchaseBonusAmountChanged:n.purchaseBonusAmountChanged,optIn:n.optIn,changeCategory:n.changeCategory,initFlashy:n.initFlashy}}