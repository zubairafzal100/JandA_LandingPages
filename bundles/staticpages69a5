var StaticPages=function(){return self.initSummaryLinkClick=function(n,t,i=15){$(t).on("click",function(t){t.preventDefault();var r=$(n),e=$(this).attr("href"),u=$(`${e}`).offset(),f=u.top;r.css("position")=="fixed"&&(f=u.top-r.height()-i);window.scrollTo(0,f)})},self.paymentTable=function(){$(".withdraw-link").click(function(){$("#deposit-table").hide();$("#withdraw-table").show();$(".deposit-link").removeClass("active");$(this).addClass("active")});$(".deposit-link").click(function(){$("#withdraw-table").hide();$("#deposit-table").show();$(".withdraw-link").removeClass("active");$(this).addClass("active")})},{initSummaryLinkClick:self.initSummaryLinkClick,paymentTable:self.paymentTable}}